<h1>Browser</h1>
<form action="/" method="GET">
  <input type="text" class="search" name="title" placeholder="Search...">
  <input type="number" name="dificultyFrom" class="difficulty" placeholder="Difficulty from...">
  <span>-</span>
  <input type="number" name="dificultyTo" class="difficulty" placeholder="Difficulty to...">
  <input type="submit" value="search">
</form>

{{#each cubes}}
<div class="cube">
  <img class="cube" src="{{imageUrl}}">
  <p class="name">{{name}}</p>
  <p><span>Difficulty level:</span> {{difficulty}}</p>
  <a class="btn" href="/details/{{_id}}">Details</a>
  <a class="btn" href="/accessory/attach/{{_id}}">Attach</a>
</div>
{{/each}}