<h1>Delete Cube</h1>
<div class="form">
    <form method="POST">
        <label for="name">Name</label>
        <input type="text" id="name" name="name" value="{{cubeData.name}}" disabled="true" />
        <label for="description">Description</label>
        <textarea id="description" name="description" disabled="true" />{{cubeData.description}}</textarea>
        <label for="image">ImageUrl</label>
        <input type="text" id="image" name="imageUrl" value="{{cubeData.imageUrl}}" disabled="true" />
        <label for="difficulty">Difficulty</label>
        <select id="difficulty" name="difficultyLevel" value="{{cubeData.difficulty}}" disabled="true" />
        {{#each difficultyArr}}
        <option value="{{value}}">{{name}}</option>
        {{/each}}
        </select>
        <input type="submit" value="Delete">
    </form>
</div>