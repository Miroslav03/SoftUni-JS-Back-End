<h1>Edit Cube</h1>
<div class="form">
    <form method="POST">
        <label for="name">Name</label>
        <input type="text" id="name" name="name" value="{{cubeData.name}}">
        <label for="description">Description</label>
        <textarea id="description" name="description">{{cubeData.description}}</textarea>
        <label for="image">ImageUrl</label>
        <input type="text" id="image" name="imageUrl" value="{{cubeData.imageUrl}}">
        <label for="difficulty">Difficulty</label>
        <select id="difficulty" name="difficulty" />
        {{#each difficultyArr}}
        <option value="{{value}}">{{name}}</option>
        {{/each}}
        </select>
        <input type="submit" value="Edit">
    </form>
</div>